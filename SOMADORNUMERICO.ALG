algoritmo "SomadorNumerico"
var
   contador, N, Soma :inteiro
inicio
      contador <- 1
      Soma <- 0
      Enquanto (contador <= 10) faça
      Escreva ("Digite um numero: ")
      Leia    (N)
      Soma <- Soma + N
      contador <- contador + 1
      Fimenquanto
      Escreva ("A soma de todos valores foi ", Soma)
      
fimalgoritmo